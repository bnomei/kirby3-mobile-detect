{
    "name": "bnomei/kirby3-mobile-detect",
    "type": "kirby-plugin",
    "version": "1.3.0",
    "license": "MIT",
    "description": "Static Class and Page Methods for using Mobile Detect Lib",
    "authors": [
        {
            "name": "Bruno Meilick",
            "email": "b@bnomei.com"
        }
    ],
    "keywords": [
        "kirby3",
        "kirby3-cms",
        "kirby3-plugin",
        "mobile-detect"
    ],
    "autoload": {
        "psr-4": {
            "Bnomei\\": "classes/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1.0",
        "getkirby/composer-installer": "^1.1",
        "mobiledetect/mobiledetectlib": "^2.8"
    }
}
